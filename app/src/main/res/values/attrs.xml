<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GlobalAttrDeclare">
        <attr name="title" format="string" />
        <attr name="subtitle" format="string" />
        <attr name="bottomBorder" format="boolean" />
    </declare-styleable>
    <declare-styleable name="TransactionInfoItemView">
        <attr name="title" />
        <attr name="value" format="string" />
        <attr name="valueSubtitle" format="string" />
        <attr name="showValueBackground" format="boolean" />
        <attr name="valueIcon" format="integer" />
    </declare-styleable>
    <declare-styleable name="FullTransactionInfoItemView">
        <attr name="infoTitle" format="string" />
        <attr name="infoValue" format="string" />
        <attr name="infoValueIcon" format="integer" />
    </declare-styleable>
    <declare-styleable name="RateDiffView">
        <attr name="negativeColor" format="color" />
        <attr name="positiveColor" format="color" />
    </declare-styleable>
    <declare-styleable name="SettingItemWithCheckmark">
        <attr name="title" />
        <attr name="subtitle" />
        <attr name="bottomBorder" />
    </declare-styleable>
</resources>
